var Acespades = function(x,y){
    fill(0, 0, 0);
    strokeWeight(3);
    noFill();
    rect(x+129,y+100,100,149);
    fill(0, 0, 0);
    spades(x+128,y+90);
    spades(x+68,y+-11);
    drawBitmoji(x+173,y+156,34);
};
//Acespades(5,-87);

