    fill(0, 0, 0);
    strokeWeight(3);
    noFill();
    rect(x+129,y+100,100,149);
    image(getImage("cute/Heart"),x+200,y+212,25,35);
    image(getImage("cute/Heart"),x+135,y+104,25,35);
    drawPerson(x+140,y+137,35);
};
