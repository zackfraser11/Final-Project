    clubs(x+28,y+-103);
    clubs(x+94,y+-35);
    image(getImage("space/9"),x+135,y+104,25,25);
    image(getImage("space/9"),x+204,y+223,25,25);
    drawPerson(x+142,y+140,34);
};
//nineclubs(145,-73);

var JClubs = function(x,y) {
    fill(255, 255, 255);
    strokeWeight(3);
    rect(x+169,y+189,100,149);
    drawBitmoji(x+204,y+278,35);
    drawBitmoji(x+235,y+210,35);
    fill(10, 10, 10);
    textSize(30);
    text("J",x+177,y+220);
    text("J",x+246,y+323);
    fill(0, 0, 0);
    clubs(x+67,y+-6);
    clubs(x+134,y+37);
};
//JClubs(-127,30);

